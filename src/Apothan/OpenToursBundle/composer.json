{
    "name": "apothan/opentoursbundle",
    "license": "GPL-2.0-only",
    "version": "0.0.14",
    "type": "symfony-bundle",
    "description": "Cool and reusable bundle",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Apothan\\OpenToursBundle\\": "src/"
        }
    },
    "require": {
        "php": ">=7.1.3",
        "symfony/flex": ">=1.0",
        "symfony/framework-bundle": ">=4.1",
        "sensio/framework-extra-bundle": ">=5.2",
        "symfony/yaml": ">=4.1",
        "doctrine/orm": "^2.8",
        "doctrine/doctrine-bundle": "2.2.3",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "symfony/form": "^5.2.3"
    },
    "config": {
        "platform": {
            "php": "7.1.3"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}